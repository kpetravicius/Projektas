<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.aloyzas.medicineReminder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.aloyzas.medicineReminder.MedicineListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
                </intent-filter>
            </activity>
        <activity
            android:name="com.example.aloyzas.medicineReminder.MedicinePagerActivity"
            android:parentActivityName="com.example.aloyzas.medicineReminder.MedicineListActivity">
        </activity>
    </application>

</manifest>